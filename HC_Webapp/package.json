{
  "name": "handicraft",
  "version": "2.0.0",
  "main": "app.js",
  "scripts": {
    "test": "node app.js",
    "sq-i": "sequelize init",
    "sq-ms": "sequelize init:migrations && sequelize init:seeders",
    "sq-mg-products": "sequelize migration:generate --name products",
    "sq-sg-products": "sequelize seed:generate --name products",
    "sq-mg-images": "sequelize migration:generate --name images",
    "sq-sg-images": "sequelize seed:generate --name images",
    "sq-mg-imagesProducts": "sequelize migration:generate --name imagesProducts",
    "sq-sg-imagesProducts": "sequelize seed:generate --name imagesProducts",
    "sq-mg": "sequelize migration:generate --name Usuario",
    "sq-sg": "sequelize seed:generate --name Usuario",
    "s-db": "sequelize db:seed:all --debug",
    "s-h": "sequelize db:seed:all --models-path",
    "s-ss": "sequelize db:seed --seed 20220826174921-Usuario",
    "s-dt": "sequelize db:seed:undo --seed UsuarioMigration",
    "sq-db": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "2": "^3.0.0",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "ejs": "^3.1.8",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "express-validator": "^6.14.2",
    "method-override": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.3",
    "normalize.css": "^8.0.1",
    "sequelize": "^6.21.4",
    "validator": "^13.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fcgrandoli"
  },
  "description": "Es un emprendimiento que pone al alcance de un click la compra de productos en los que interviene el diseño de articulos en vidrio, con los mejores estándares de especificación y calidad.",
  "devDependencies": {
    "sequelize-cli": "^6.4.1"
  }
}
