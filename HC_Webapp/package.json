{
  "name": "handicraft",
  "version": "2.0.0",
  "main": "app.js",
  "scripts": {
    "test": "node app.js",
    "sq-i": "sequelize init",
    "sq-ms": "sequelize init:migrations && sequelize init:seeders",
    "sq-mg": "sequelize migration:generate --name usersList",
    "sq-sg": "sequelize seed:generate --name usersList",
    "sq-db": "sequelize db:drop && sequelize db:create && sequelize db:migrate && sequelize db:seed:all",
    "s-db": "sequelize db:seed:all --debug"
  },
